{
  "name": "comfyui-mcp",
  "version": "0.2.0",
  "description": "MCP server for ComfyUI â€” workflow execution, visualization, composition, registry, and skill generation",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "comfyui-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:integration": "cross-env COMFYUI_INTEGRATION=true vitest run",
    "lint": "tsc --noEmit",
    "postinstall": "node scripts/postinstall.mjs",
    "generations:stats": "node scripts/generation-stats.mjs",
    "release": "npm version patch && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:major": "npm version major && git push --follow-tags"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
